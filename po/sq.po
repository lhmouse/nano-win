msgstr "Caktoni drejtori veprimesh"

#: src/nano.c:619
msgid "Preserve XON (^Q) and XOFF (^S) keys"
msgstr "Ruaj tastet XON (^Q) dhe XOFF (^S)"

#: src/nano.c:621
msgid "Show a position+portion indicator"
msgstr "Shfaq një tregues pozicioni+pjese"

#: src/nano.c:624
msgid "-r <number>"
msgstr "-r <numër>"

#: src/nano.c:624
msgid "--fill=<number>"
